{
  "name": "nodejs-practice",
  "version": "1.0.0",
  "description": "The source use to practice features in nodejs",
  "scripts": {
    "start:dev": "nodemon src/index.ts",
    "start:nodemon": "nodemon",
    "start:cluster": "nodemon src/threads/cluster/index.ts",
    "start:debug": "node --inspect --require ts-node/register src/debug/index.ts",
    "start:swagger": "ts-node src/swagger/app.ts",
    "format": "prettier --write .",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "repository": {
